<Page x:Class="RailWay.AllPages.MyTicketsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:RailWay.AllPages"
      mc:Ignorable="d"
      Title="Билеты" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <DataGrid x:Name="TicketsDataGrid">
                <DataGrid.Columns>
                    <DataGridTextColumn Header="Поезд"/>
                    <DataGridTextColumn Header="Время"/>
                </DataGrid.Columns>
            </DataGrid>
        </StackPanel>
    </Grid>
</Page>
